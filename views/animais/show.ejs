<% include ../header %>
        		<div class="row marketing">
                    <section class="animal">
                            <h1 class="pull-left" >Animal</h1>
                                <% if (animais) {%>
                                    <% animais.forEach(function(animal){ %>
                                        <header class="user-header">    
                                           <div class="actions pull-right">
                                                <form action="/animal/<%-animal._id%>" method="post">
                                                    <a href="/animal/<%-animal._id%>/edit" class="btn btn-default">Editar</a>
                                                </form>
                                            </div>
                                        </header>
                                    <article>
                                            <strong>Nome: </strong><%- animal.name %><br>
                                            <strong>E-Mail: </strong> <%- animal.email %>
                                            <strong>E-Mail: </strong> <%- animal.texto %>
                                    </article>
                                        <div class="row">
                                            <hr />
                                        </div>
                                    <% }) %>
                                <% }else{ %>
                                    <h2>Nenhum manejo encontrado</h2>
                                <% } %>
                    </section>
                </div>
<% include ../footer %>






