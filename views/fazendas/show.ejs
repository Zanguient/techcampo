<% include ../header %>
        		<div class="row marketing">
                    <section class="fazenda">
                    <header class="user-header">
                    <h1 class="pull-left" >Propriedade</h1>
                    <div class="col-lg-12">
                        <% if (fazendas) {%>
                            <% fazendas.forEach(function(fazenda){ %>
                                <div class="actions pull-right">
                                    <form action="/fazenda/<%-fazenda._id%>" method="post">
                                        <a href="/fazenda/<%-fazenda._id%>/edit" class="btn btn-default">Editar</a>
                                    </form>
                                </div>
                    </header>
                        <article>
                            <strong>Nome: </strong><%- fazenda.name %><br>
                            <strong>E-Mail: </strong> <%- fazenda.email %>
                        </article>
                        </section>
	                </div>
                    <div class="row">
                    <hr />
                    </div>
                    <% }) %>
                        <% }else{ %>
                            <h2>Nenhum manejo encontrado</h2>
                        <% } %>
                </div>
                
				
<% include ../footer %>